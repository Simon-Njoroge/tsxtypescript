{
  "compilerOptions": {
    
    "target": "es2016",                                  
    "module": "NodeNext",                                
    "rootDir": "./src",                                  
     "moduleResolution": "NodeNext",                     
    "outDir": "./tojs",                                    
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    
    "skipLibCheck": true                                 
  },
  "include": ["src/**/*"],
  "exclude": ["node_module"]
}
